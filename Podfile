# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'NavVis' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for NavVis
  
  # Fabric
  pod 'Fabric'
  pod 'Crashlytics'
  
  # Side menu
  pod 'SWRevealViewController'
  
  # HUD
  pod 'FTIndicator'

  # QR Code
  pod 'swiftScan'

  # Number Pad
  pod 'APNumberPad'
  
  # Timer
  pod 'SwiftyTimer'
  
  # Sounds
  pod 'SwiftySound'
  
  # Pop up
  pod 'MIBlurPopup'
end
